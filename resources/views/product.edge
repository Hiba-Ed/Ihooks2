<style>
  border: 1px solid #666;
  td {
    border: 1px solid #666;
    padding: 10px;
  }
</style>
<h1>All Products</h1>
<table>
  <tr>
    <th>#</th>
    <th>Name</th>
    <th>Price</th>
    <th>Create Datetime</th>
  </tr>
  @each(p in products) 
  <tr>
    <td>{{p.id}}</td>
    <td>{{p.name}}</td>
    <td>{{p.price}}</td>
    <td>{{ getRelativeDate(p.created_datetime) }}</td>
  </tr>
  @endeach
</table>